{
    "manifest_version": 3,
    "name": "Your Extension Name",
    "version": "1.0",
    "description": "Your extension description",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "logo192.png",
          "favicon.ico"
        ],
        "matches": ["<all_urls>"]
      }
    ],
    "permissions": [
      "activeTab"
    ]
  }
  